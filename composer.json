{
	"name": "finesse/micro-db",
	"description": "A convenient PDO wrapper for using pure SQL with bindings",
	"keywords": ["sql", "pdo", "database", "mysql"],
	"type": "library",
	"homepage": "https://github.com/FinesseRus/MicroDB",
	"license": "MIT",
	"authors": [
		{
			"name": "Surgie Finesse",
			"email": "finesserus@gmail.com"
		}
	],
	"require": {
		"php": ">=7.0",
		"ext-pdo": "*"
	},
	"require-dev": {
		"phpunit/phpunit": "^6.4",
		"mockery/mockery": "^1.0"
	},
	"autoload": {
		"psr-4": {
			"Finesse\\MicroDB\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Finesse\\MicroDB\\Tests\\": "tests/"
		}
	},
	"scripts": {
		"test": "vendor/bin/phpunit"
	}
}
